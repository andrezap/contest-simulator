{% extends 'base.html.twig' %}

{% block body %}
  <main role="main" class="inner cover">
    {% if history.winners is not empty %}
      <h1 class="cover-heading">Last 5 winners!</h1>
      <ul>
        {% for winner in history.winners %}
          <li>{{ winner[0].name }} - {{ winner[1] }} points</li>
        {% endfor %}
      </ul>

      <h1 class="cover-heading">TOP CONTESTANT</h1>
      {{ history.highestScore[0].name }} -
      {{ history.highestScore['fs'] }} points
    {% else %}
      No history
    {% endif %}
  </main>
{% endblock body %}